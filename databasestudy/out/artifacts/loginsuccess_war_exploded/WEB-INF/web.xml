<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>ServletContex</servlet-name>
        <servlet-class>com.itheima.web.ServletContex</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletContex</servlet-name>
        <url-pattern>/ServletContex</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FailServlet</servlet-name>
        <servlet-class>com.itheima.web.FailServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FailServlet</servlet-name>
        <url-pattern>/FailServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SuccesServlet</servlet-name>
        <servlet-class>com.itheima.web.SuccesServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SuccesServlet</servlet-name>
        <url-pattern>/SuccesServlet</url-pattern>
    </servlet-mapping>
</web-app>